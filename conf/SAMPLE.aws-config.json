{
    "extends":[],
    "services": {
        "default_settings":{
            "params":{
                "access_key_id":"XXX",
                "secret_access_key":"XXX",
                "use_sys_log":true
            }
        },
        "dynamo_db": {
            "host":"dynamodb.us-east-1.amazonaws.com",
            "zone":"us-east-1",
            "scheme":"http",
            "port":80,
            "keepalive":true,
            "iam": {
                "use_iam":false,
                "role_provider":"file",
                "access_key":"role_access_key",
                "secret_key":"role_secret_key",
                "token":"role_token",
                "base_dir":"/dir/where/you/update/role_files",
                "watch":true
            }
        }
    }
}
